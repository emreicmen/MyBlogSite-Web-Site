@using MyEvernote.BussinessLayer
@using MyEvernote.Entites
@using MyEvernote.WebApp.Models

@{
    List<Category> list = CacheHelper.GetCategoriesFromCache();
}
<div class="list-group">
    <a href="@Url.Action("Index","Home")" class="list-group-item bg-primary text-light"> Tümü</a>
    @foreach (Category cat in list)
    {
        <a href="/Home/ByCategory/@cat.Id" title="@cat.Description" class="list-group-item bg-dark text-white"> @cat.Title</a>
    }
</div>
